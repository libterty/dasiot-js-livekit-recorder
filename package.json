{
  "name": "livekit-recording-project",
  "version": "1.0.0",
  "description": "A TypeScript Node.js project for recording with Livekit Track Egress",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js"
  },
  "dependencies": {
    "@livekit/rtc-node": "^0.9.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "livekit-server-sdk": "^1.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.0"
  }
}
